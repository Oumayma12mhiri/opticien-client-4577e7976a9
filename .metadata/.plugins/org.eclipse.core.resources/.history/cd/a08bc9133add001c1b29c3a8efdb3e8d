    @Override
    public List<VenteDto> fromEntitiesToDtoList(List<Vente> i) {
        if ( i == null ) {
            return null;
        }

        List<VenteDto> list = new ArrayList<VenteDto>( i.size() );
        for ( Vente vente : i ) {
            list.add( fromEntityToDto( vente ) );
